{
  "name": "bb-webpack-frontend",
  "version": "0.0.2",
  "description": "bb webpack configuration",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm run clear && npm install",
    "build": "webpack --mode production --colors --progress",
    "clear": "rimraf dist",
    "start": "webpack-dev-server --mode development --hot --colors --progress --open",
    "stats:build": "webpack --mode development --config webpack.config.js --profile --json > stats.json",
    "stats:view": "webpack-bundle-analyzer stats.json"
  },
  "author": "blindlybright",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.2",
    "@babel/preset-env": "^7.5.2",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "browserslist": [
    "last 2 version",
    "ie >= 8",
    "> 1%"
  ]
}
